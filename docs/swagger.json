{
    "swagger": "2.0",
    "info": {
        "description": "An example template of a Golang simple backend API using Fiber",
        "title": "go-api-starter",
        "contact": {
            "name": "Aaron Saikovski"
        },
        "license": {
            "name": "MIT"
        },
        "version": "0.1"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/ping": {
            "get": {
                "description": "get the status of server.",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Show the status of server - Ping/Pong",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/weatherforecast": {
            "get": {
                "description": "Sample random weatherforecast data",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "weather"
                ],
                "summary": "Sample weatherforecast",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    }
}